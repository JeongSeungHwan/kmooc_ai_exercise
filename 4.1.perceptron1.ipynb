    "np.random.seed(0) #난수 고정\n",
    "x = np.arange(4).reshape(-1,1)\n",
    "w = np.array(np.random.random(4)).reshape(-1,1)\n",
    "z = np.dot(w.T,x).squeeze() #squeeze -> 배열 안의 값만 가져오기\n",
    "print(z)"
   ]
